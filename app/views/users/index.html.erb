<div class="container">
  <div class="row">
    <div class="col-3"></div>
    <div class="col-9">
      <table class="table">
        <thead>
          <tr>
            <th>name</th>
          </tr>
        </thead>
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <td><%= user.name %></td>
              <td><%= link_to "show", user_path(user) %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>