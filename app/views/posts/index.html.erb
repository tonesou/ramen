<div class="container">
  <div class="row">
    <div class="col-3"></div>
    <div class="col-9">
      <h1>みんなのラーメン</h1>
      <table class="table">
        <thead>
          <tr>
            <th>shop</th>
            <th>explain</th>
          </tr>
        </thead>
        <tbody>
          <% @posts.each do |posts| %>
            <tr>
              <td><%= posts.shop %></td>
              <td><%= posts.explain %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>