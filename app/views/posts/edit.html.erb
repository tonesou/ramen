<div class="container">
  <div class="row">
    <div class="col-3"></div>
    <div class="col-9">
      <h1>edit</h1>
      <%= from_with model: @post do |f| %>
        <h4>image</h4>
        <%= f.file_field :image %>
        <h4>shop</h4>
        <%= f.text_field :shop %>
        <h4>explain</h4>
        <%= f.text_area :explain %>
        <%= f.submit 'Update Post' %>
      <% end %>
    </div>
  </div>
</div>